setup <- function() {
    size(100, 100)
    bot = loadShape(paste("https://raw.githubusercontent.com", "/gaocegege/Processing.R/master/raw-docs", 
        "/img/examples/bot1.svg", sep = ""))
}

draw <- function() {
    shapeMode(CENTER)
    shape(bot, 35, 35, 50, 50)
    shapeMode(CORNER)
    shape(bot, 35, 35, 50, 50)
}
