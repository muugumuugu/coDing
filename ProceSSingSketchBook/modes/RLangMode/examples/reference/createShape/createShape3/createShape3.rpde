settings <- function() {
    size(100, 100)
}

setup <- function() {
    # Creating the PShape as a square. The numeric arguments are
    # similar to rect().
    s = createShape()
    s$beginShape(TRIANGLE_STRIP)
    s$vertex(30, 75)
    s$vertex(40, 20)
    s$vertex(50, 75)
    s$vertex(60, 20)
    s$vertex(70, 75)
    s$vertex(80, 20)
    s$vertex(90, 75)
    s$endShape()
}

draw <- function() {
    shape(s, 0, 0)
}
